{
  "name": "barotrauma-recipes",
  "version": "0.0.1",
  "author": {
    "name": "Akuukis",
    "url": "https://github.com/Akuukis"
  },
  "scripts": {
    "start": "yarn build && yarn serve",
    "build": "yarn exec ts-node scripts/parse.ts",
    "deploy": "bash -c 'source .env; scp -r dist/* @$FTP_HOSTNAME:$FTP_PATH'",
    "watch": "yarn concurrently \"yarn:watch:*\"",
    "watch:dist": "yarn exec serve dist",
    "watch:src": "yarn tsc --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "fast-xml-parser": "^4.0.0-beta.6",
    "path": "^0.12.7",
    "serve": "^13.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "concurrent": "^0.3.2",
    "concurrently": "^6.4.0"
  }
}
